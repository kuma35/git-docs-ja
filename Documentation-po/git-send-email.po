#: ../Documentation-sedout/git-send-email.txt:526
#, no-wrap
msgid "GIT"
msgstr "GIT"

#. type: Plain text
#: ../Documentation-sedout/git-send-email.txt:527
msgid "Part of the linkgit:git[1] suite"
msgstr "Part of the linkgit:git[1] suite"

