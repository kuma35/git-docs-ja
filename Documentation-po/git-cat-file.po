"'git cat-file' (-e | -p) <object>\n"
"'git cat-file' (-t | -s) [--allow-unknown-type] <object>\n"
"'git cat-file' (--batch | --batch-check | --batch-command) [--batch-all-objects]\n"
"\t     [--buffer] [--follow-symlinks] [--unordered]\n"
"\t     [--textconv | --filters] [-z]\n"
