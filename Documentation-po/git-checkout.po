#: ../Documentation-sedout/git-checkout.txt:618
msgid "linkgit:git-switch[1], linkgit:git-restore[1]"
msgstr "linkgit:git-switch[1], linkgit:git-restore[1]"

#. type: Title -
#: ../Documentation-sedout/git-checkout.txt:620
#, no-wrap
msgid "GIT"
msgstr "GIT"

#. type: Plain text
#: ../Documentation-sedout/git-checkout.txt:621
msgid "Part of the linkgit:git[1] suite"
msgstr "Part of the linkgit:git[1] suite"
