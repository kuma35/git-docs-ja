#. type: Labeled list
#: ../Documentation/diff-options.txt:153
#, no-wrap
msgid "--histogram"
msgstr ""

#. type: Plain text
#: ../Documentation/diff-options.txt:155
msgid "Generate a diff using the \"histogram diff\" algorithm."
msgstr ""

#. type: Labeled list
#: ../Documentation/diff-options.txt:156
#, no-wrap
msgid "--anchored=<text>"
msgstr ""

#. type: Plain text
#: ../Documentation/diff-options.txt:158
msgid "Generate a diff using the \"anchored diff\" algorithm."
