msgstr ""

#. type: Labeled list
#: ../Documentation/diff-options.txt:172
#, no-wrap
msgid "`minimal`"
msgstr ""

#. type: Labeled list
#: ../Documentation/diff-options.txt:175
#, no-wrap
msgid "`patience`"
msgstr ""

#. type: Plain text
#: ../Documentation/diff-options.txt:177
msgid "Use \"patience diff\" algorithm when generating patches."
msgstr ""

#. type: Labeled list
#: ../Documentation/diff-options.txt:177
#, no-wrap
msgid "`histogram`"
msgstr ""

#. type: Plain text
#: ../Documentation/diff-options.txt:180
msgid ""
"This algorithm extends the patience algorithm to \"support low-occurrence "
"common elements\"."
msgstr ""

#. type: Plain text
#: ../Documentation/diff-options.txt:185
