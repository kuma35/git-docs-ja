msgid "linkgit:gitnamespaces[7]"
msgstr "linkgit:gitnamespaces[7]"

#. type: Title -
#: ../Documentation-sedout/git-upload-pack.txt:64
#, no-wrap
msgid "GIT"
msgstr "GIT"

#. type: Plain text
