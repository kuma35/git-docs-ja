#. type: Plain text
#: ../Documentation/git-cvsserver.txt:7
msgid "git-cvsserver - A CVS server emulator for Git"
msgstr ""

#. type: Title -
#: ../Documentation/git-cvsserver.txt:9
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: ../Documentation/git-cvsserver.txt:12
msgid "SSH:"
msgstr ""

#. type: Plain text
#: ../Documentation/git-cvsserver.txt:16
#, no-wrap
msgid ""
"export CVS_SERVER=\"git cvsserver\"\n"
"'cvs' -d :ext:user@server/path/repo.git co <HEAD_name>\n"
msgstr ""

#. type: Plain text
#: ../Documentation/git-cvsserver.txt:18
msgid "pserver (/etc/inetd.conf):"
msgstr ""

#. type: Plain text
#: ../Documentation/git-cvsserver.txt:21
#, no-wrap
msgid ""
"cvspserver stream tcp nowait nobody /usr/bin/git-cvsserver git-cvsserver "
"pserver\n"
msgstr ""

#. type: Plain text
#: ../Documentation/git-cvsserver.txt:23
msgid "Usage:"
msgstr ""

#. type: Plain text
#: ../Documentation/git-cvsserver.txt:26
#, no-wrap
msgid "'git-cvsserver' [<options>] [pserver|server] [<directory> ...]\n"
msgstr ""

