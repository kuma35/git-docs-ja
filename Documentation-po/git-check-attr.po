#. type: Title =
#: ../Documentation/git-check-attr.txt:2
#, no-wrap
msgid "git-check-attr(1)"
msgstr ""

#. type: Title -
#: ../Documentation/git-check-attr.txt:5
#, no-wrap
msgid "NAME"
msgstr ""

#. type: Plain text
#: ../Documentation/git-check-attr.txt:7
msgid "git-check-attr - Display gitattributes information"
msgstr ""

#. type: Title -
#: ../Documentation/git-check-attr.txt:10
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: ../Documentation/git-check-attr.txt:14
#, no-wrap
msgid ""
"'git check-attr' [-a | --all | <attr>...] [--] <pathname>...\n"
"'git check-attr' --stdin [-z] [-a | --all | <attr>...]\n"
msgstr ""

#. type: Title -
#: ../Documentation/git-check-attr.txt:16
#, no-wrap
msgid "DESCRIPTION"
msgstr ""

#. type: Plain text
#: ../Documentation/git-check-attr.txt:19
msgid ""
"For every pathname, this command will list if each attribute is "
"'unspecified', 'set', or 'unset' as a gitattribute on that pathname."
msgstr ""

#. type: Title -
#: ../Documentation/git-check-attr.txt:21
#, no-wrap
msgid "OPTIONS"
msgstr ""

#. type: Labeled list
#: ../Documentation/git-check-attr.txt:22
#, no-wrap
msgid "-a, --all"
msgstr ""

#. type: Plain text
#: ../Documentation/git-check-attr.txt:26
msgid ""
"List all attributes that are associated with the specified paths.  If this "
"option is used, then 'unspecified' attributes will not be included in the "
"output."
msgstr ""

#. type: Labeled list
#: ../Documentation/git-check-attr.txt:27
#, no-wrap
msgid "--cached"
msgstr ""

#. type: Plain text
#: ../Documentation/git-check-attr.txt:29
msgid "Consider `.gitattributes` in the index only, ignoring the working tree."
msgstr ""

#. type: Labeled list
#: ../Documentation/git-check-attr.txt:30
#, no-wrap
msgid "--stdin"
msgstr ""

#. type: Plain text
#: ../Documentation/git-check-attr.txt:33
msgid ""
"Read pathnames from the standard input, one per line, instead of from the "
"command-line."
msgstr ""

#. type: Labeled list
#: ../Documentation/git-check-attr.txt:34
#, no-wrap
msgid "-z"
msgstr ""

#. type: Plain text
#: ../Documentation/git-check-attr.txt:38
msgid ""
"The output format is modified to be machine-parsable.  If `--stdin` is also "
"given, input paths are separated with a NUL character instead of a linefeed "
"character."
msgstr ""

#. type: Labeled list
#: ../Documentation/git-check-attr.txt:39
#, no-wrap
msgid "\\--"
msgstr ""

#. type: Plain text
#: ../Documentation/git-check-attr.txt:42
msgid ""
"Interpret all preceding arguments as attributes and all following arguments "
"as path names."
msgstr ""

#. type: Plain text
#: ../Documentation/git-check-attr.txt:46
msgid ""
"If none of `--stdin`, `--all`, or `--` is used, the first argument will be "
"treated as an attribute and the rest of the arguments as pathnames."
msgstr ""

#. type: Title -
#: ../Documentation/git-check-attr.txt:48
#, no-wrap
msgid "OUTPUT"
msgstr ""

#. type: Plain text
#: ../Documentation/git-check-attr.txt:52
msgid "The output is of the form: <path> COLON SP <attribute> COLON SP <info> LF"
msgstr ""

#. type: Plain text
#: ../Documentation/git-check-attr.txt:55
msgid ""
"unless `-z` is in effect, in which case NUL is used as delimiter: <path> NUL "
"<attribute> NUL <info> NUL"
msgstr ""

#. type: Plain text
#: ../Documentation/git-check-attr.txt:59
msgid ""
"<path> is the path of a file being queried, <attribute> is an attribute "
"being queried and <info> can be either:"
msgstr ""

#. type: Labeled list
#: ../Documentation/git-check-attr.txt:60
#, no-wrap
msgid "'unspecified'"
msgstr ""

#. type: Plain text
#: ../Documentation/git-check-attr.txt:61
msgid "when the attribute is not defined for the path."
msgstr ""

#. type: Labeled list
#: ../Documentation/git-check-attr.txt:61
#, no-wrap
msgid "'unset'"
msgstr ""

#. type: Plain text
#: ../Documentation/git-check-attr.txt:62
msgid "when the attribute is defined as false."
msgstr ""

#. type: Labeled list
#: ../Documentation/git-check-attr.txt:62
#, no-wrap
msgid "'set'"
msgstr ""

#. type: Plain text
#: ../Documentation/git-check-attr.txt:63
msgid "when the attribute is defined as true."
msgstr ""

#. type: Labeled list
#: ../Documentation/git-check-attr.txt:63
#, no-wrap
msgid "<value>"
msgstr ""

#. type: Plain text
#: ../Documentation/git-check-attr.txt:64
msgid "when a value has been assigned to the attribute."
msgstr ""

#. type: Plain text
#: ../Documentation/git-check-attr.txt:69
msgid ""
"Buffering happens as documented under the `GIT_FLUSH` option in "
"linkgit:git[1].  The caller is responsible for avoiding deadlocks caused by "
"overfilling an input buffer or reading from an empty output buffer."
msgstr ""

#. type: Title -
#: ../Documentation/git-check-attr.txt:71
#, no-wrap
msgid "EXAMPLES"
msgstr ""

#. type: Plain text
#: ../Documentation/git-check-attr.txt:74
msgid "In the examples, the following '.gitattributes' file is used:"
msgstr ""

#. type: delimited block -
#: ../Documentation/git-check-attr.txt:78
#, no-wrap
msgid ""
"*.java diff=java -crlf myAttr\n"
"NoMyAttr.java !myAttr\n"
"README caveat=unspecified\n"
msgstr ""

#. type: Plain text
#: ../Documentation/git-check-attr.txt:81
msgid "Listing a single attribute:"
msgstr ""

#. type: delimited block -
#: ../Documentation/git-check-attr.txt:84
#, no-wrap
msgid ""
"$ git check-attr diff org/example/MyClass.java\n"
"org/example/MyClass.java: diff: java\n"
msgstr ""

#. type: Plain text
#: ../Documentation/git-check-attr.txt:87
msgid "Listing multiple attributes for a file:"
msgstr ""

#. type: delimited block -
#: ../Documentation/git-check-attr.txt:92
#, no-wrap
msgid ""
"$ git check-attr crlf diff myAttr -- org/example/MyClass.java\n"
"org/example/MyClass.java: crlf: unset\n"
"org/example/MyClass.java: diff: java\n"
"org/example/MyClass.java: myAttr: set\n"
msgstr ""

#. type: Plain text
#: ../Documentation/git-check-attr.txt:95
msgid "Listing all attributes for a file:"
msgstr ""

#. type: delimited block -
#: ../Documentation/git-check-attr.txt:99
#, no-wrap
msgid ""
"$ git check-attr --all -- org/example/MyClass.java\n"
"org/example/MyClass.java: diff: java\n"
"org/example/MyClass.java: myAttr: set\n"
msgstr ""

#. type: Plain text
#: ../Documentation/git-check-attr.txt:102
msgid "Listing an attribute for multiple files:"
msgstr ""

#. type: delimited block -
#: ../Documentation/git-check-attr.txt:106
#, no-wrap
msgid ""
"$ git check-attr myAttr -- org/example/MyClass.java "
"org/example/NoMyAttr.java\n"
"org/example/MyClass.java: myAttr: set\n"
"org/example/NoMyAttr.java: myAttr: unspecified\n"
msgstr ""

#. type: Plain text
#: ../Documentation/git-check-attr.txt:109
msgid "Not all values are equally unambiguous:"
msgstr ""

#. type: delimited block -
#: ../Documentation/git-check-attr.txt:112
#, no-wrap
msgid ""
"$ git check-attr caveat README\n"
"README: caveat: unspecified\n"
msgstr ""

