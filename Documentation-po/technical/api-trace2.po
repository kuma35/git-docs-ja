#, no-wrap
msgid "Child Processes"
msgstr "Child Processes"

#. type: Plain text
#: ../Documentation-sedout/technical/api-trace2.txt:890
msgid "Wrap code spawning child processes."
msgstr "子プロセスを生成するコードをラップします。"

#. type: delimited block -
#: ../Documentation-sedout/technical/api-trace2.txt:898
#, no-wrap
msgid ""
"void run_child(...)\n"
"{\n"
"\tint child_exit_code;\n"
"\tstruct child_process cmd = CHILD_PROCESS_INIT;\n"
"\t...\n"
"\tcmd.trace2_child_class = \"editor\";\n"
msgstr ""
"void run_child(...)\n"
"{\n"
"\tint child_exit_code;\n"
"\tstruct child_process cmd = CHILD_PROCESS_INIT;\n"
"\t...\n"
"\tcmd.trace2_child_class = \"editor\";\n"

#. type: delimited block -
#: ../Documentation-sedout/technical/api-trace2.txt:903
#, no-wrap
msgid ""
"\ttrace2_child_start(&cmd);\n"
"\tchild_exit_code = spawn_child_and_wait_for_it();\n"
"\ttrace2_child_exit(&cmd, child_exit_code);\n"
"}\n"
msgstr ""
"\ttrace2_child_start(&cmd);\n"
"\tchild_exit_code = spawn_child_and_wait_for_it();\n"
"\ttrace2_child_exit(&cmd, child_exit_code);\n"
"}\n"

