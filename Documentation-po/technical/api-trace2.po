msgid ""
"{\n"
"\t\"event\":\"data\",\n"
"\t...\n"
"\t\"repo\":1,              # optional\n"
"\t\"t_abs\":0.024107,      # absolute elapsed time\n"
"\t\"t_rel\":0.001031,      # elapsed time in region/thread\n"
"\t\"nesting\":2,           # region stack depth\n"
"\t\"category\":\"index\",\n"
"\t\"key\":\"read/cache_nr\",\n"
"\t\"value\":\"3552\"\n"
"}\n"
msgstr ""
"{\n"
"\t\"event\":\"data\",\n"
"\t...\n"
"\t\"repo\":1,              # optional\n"
"\t\"t_abs\":0.024107,      # absolute elapsed time\n"
"\t\"t_rel\":0.001031,      # elapsed time in region/thread\n"
"\t\"nesting\":2,           # region stack depth\n"
"\t\"category\":\"index\",\n"
"\t\"key\":\"read/cache_nr\",\n"
"\t\"value\":\"3552\"\n"
"}\n"

#. type: Plain text
#: ../Documentation-sedout/technical/api-trace2.txt:800
msgid "The \"value\" field may be an integer or a string."
msgstr "「value」フィールドは整数または文字列です。"

