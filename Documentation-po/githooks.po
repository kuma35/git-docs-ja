#: ../Documentation-sedout/githooks.txt:702
#, no-wrap
msgid "GIT"
msgstr ""

#. type: Plain text
#: ../Documentation-sedout/githooks.txt:703
msgid "Part of the linkgit:git[1] suite"
msgstr ""
