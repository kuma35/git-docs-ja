#. type: Title =
#: ../Documentation/git-stripspace.txt:2
#, no-wrap
msgid "git-stripspace(1)"
msgstr ""

#. type: Title -
#: ../Documentation/git-stripspace.txt:5
#, no-wrap
msgid "NAME"
msgstr ""

#. type: Plain text
#: ../Documentation/git-stripspace.txt:7
msgid "git-stripspace - Remove unnecessary whitespace"
msgstr ""

#. type: Title -
#: ../Documentation/git-stripspace.txt:10
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: ../Documentation/git-stripspace.txt:14
#, no-wrap
msgid ""
"'git stripspace' [-s | --strip-comments]\n"
"'git stripspace' [-c | --comment-lines]\n"
msgstr ""

#. type: Title -
#: ../Documentation/git-stripspace.txt:16
#, no-wrap
msgid "DESCRIPTION"
msgstr ""

#. type: Plain text
#: ../Documentation/git-stripspace.txt:21
msgid ""
"Read text, such as commit messages, notes, tags and branch descriptions, "
"from the standard input and clean it in the manner used by Git."
msgstr ""

#. type: Plain text
#: ../Documentation/git-stripspace.txt:23
msgid "With no arguments, this will:"
msgstr ""

#. type: Plain text
#: ../Documentation/git-stripspace.txt:25
msgid "remove trailing whitespace from all lines"
msgstr ""

#. type: Plain text
#: ../Documentation/git-stripspace.txt:26
msgid "collapse multiple consecutive empty lines into one empty line"
msgstr ""

#. type: Plain text
#: ../Documentation/git-stripspace.txt:27
msgid "remove empty lines from the beginning and end of the input"
msgstr ""

#. type: Plain text
#: ../Documentation/git-stripspace.txt:28
msgid "add a missing '\\n' to the last line if necessary."
msgstr ""

#. type: Plain text
#: ../Documentation/git-stripspace.txt:31
msgid ""
"In the case where the input consists entirely of whitespace characters, no "
"output will be produced."
msgstr ""

#. type: Plain text
#: ../Documentation/git-stripspace.txt:35
#, no-wrap
msgid ""
"*NOTE*: This is intended for cleaning metadata, prefer the "
"`--whitespace=fix`\n"
"mode of linkgit:git-apply[1] for correcting whitespace of patches or files "
"in\n"
"the repository.\n"
msgstr ""

#. type: Title -
#: ../Documentation/git-stripspace.txt:37
#, no-wrap
msgid "OPTIONS"
msgstr ""

#. type: Labeled list
#: ../Documentation/git-stripspace.txt:38
#, no-wrap
msgid "-s"
msgstr ""

#. type: Labeled list
#: ../Documentation/git-stripspace.txt:39
#, no-wrap
msgid "--strip-comments"
msgstr ""

#. type: Plain text
#: ../Documentation/git-stripspace.txt:41
msgid "Skip and remove all lines starting with comment character (default '#')."
msgstr ""

#. type: Labeled list
#: ../Documentation/git-stripspace.txt:42
#, no-wrap
msgid "-c"
msgstr ""

#. type: Labeled list
#: ../Documentation/git-stripspace.txt:43
#, no-wrap
msgid "--comment-lines"
msgstr ""

#. type: Plain text
#: ../Documentation/git-stripspace.txt:47
msgid ""
"Prepend comment character and blank to each line. Lines will automatically "
"be terminated with a newline. On empty lines, only the comment character "
"will be prepended."
msgstr ""

#. type: Title -
#: ../Documentation/git-stripspace.txt:49
#, no-wrap
msgid "EXAMPLES"
msgstr ""

#. type: Plain text
#: ../Documentation/git-stripspace.txt:52
msgid "Given the following noisy input with '$' indicating the end of a line:"
msgstr ""

#. type: delimited block -
#: ../Documentation/git-stripspace.txt:65
#, no-wrap
msgid ""
"|A brief introduction   $\n"
"|   $\n"
"|$\n"
"|A new paragraph$\n"
"|# with a commented-out line    $\n"
"|explaining lots of stuff.$\n"
"|$\n"
"|# An old paragraph, also commented-out. $\n"
"|      $\n"
"|The end.$\n"
"|  $\n"
msgstr ""

#. type: Plain text
#: ../Documentation/git-stripspace.txt:68
msgid "Use 'git stripspace' with no arguments to obtain:"
msgstr ""

#. type: delimited block -
#: ../Documentation/git-stripspace.txt:79
#, no-wrap
msgid ""
"|A brief introduction$\n"
"|$\n"
"|A new paragraph$\n"
"|# with a commented-out line$\n"
"|explaining lots of stuff.$\n"
"|$\n"
"|# An old paragraph, also commented-out.$\n"
"|$\n"
"|The end.$\n"
msgstr ""

#. type: Plain text
#: ../Documentation/git-stripspace.txt:82
msgid "Use 'git stripspace --strip-comments' to obtain:"
msgstr ""

#. type: delimited block -
#: ../Documentation/git-stripspace.txt:90
#, no-wrap
msgid ""
"|A brief introduction$\n"
"|$\n"
"|A new paragraph$\n"
"|explaining lots of stuff.$\n"
"|$\n"
"|The end.$\n"
msgstr ""

