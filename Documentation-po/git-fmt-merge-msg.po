#. type: Title =
#: ../Documentation/git-fmt-merge-msg.txt:2
#, no-wrap
msgid "git-fmt-merge-msg(1)"
msgstr ""

#. type: Title -
#: ../Documentation/git-fmt-merge-msg.txt:5
#, no-wrap
msgid "NAME"
msgstr ""

#. type: Plain text
#: ../Documentation/git-fmt-merge-msg.txt:7
msgid "git-fmt-merge-msg - Produce a merge commit message"
msgstr ""

#. type: Title -
#: ../Documentation/git-fmt-merge-msg.txt:10
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: ../Documentation/git-fmt-merge-msg.txt:14
#, no-wrap
msgid ""
"'git fmt-merge-msg' [-m <message>] [--log[=<n>] | --no-log]\n"
"'git fmt-merge-msg' [-m <message>] [--log[=<n>] | --no-log] -F <file>\n"
msgstr ""

#. type: Title -
#: ../Documentation/git-fmt-merge-msg.txt:16
#, no-wrap
msgid "DESCRIPTION"
msgstr ""

#. type: Plain text
#: ../Documentation/git-fmt-merge-msg.txt:20
msgid ""
"Takes the list of merged objects on stdin and produces a suitable commit "
"message to be used for the merge commit, usually to be passed as the "
"'<merge-message>' argument of 'git merge'."
msgstr ""

#. type: Plain text
#: ../Documentation/git-fmt-merge-msg.txt:23
msgid ""
"This command is intended mostly for internal use by scripts automatically "
"invoking 'git merge'."
msgstr ""

#. type: Title -
#: ../Documentation/git-fmt-merge-msg.txt:25
#, no-wrap
msgid "OPTIONS"
msgstr ""

#. type: Labeled list
#: ../Documentation/git-fmt-merge-msg.txt:27
#, no-wrap
msgid "--log[=<n>]"
msgstr ""

#. type: Plain text
#: ../Documentation/git-fmt-merge-msg.txt:33
msgid ""
"In addition to branch names, populate the log message with one-line "
"descriptions from the actual commits that are being merged.  At most <n> "
"commits from each merge parent will be used (20 if <n> is omitted).  This "
"overrides the `merge.log` configuration variable."
msgstr ""

#. type: Labeled list
#: ../Documentation/git-fmt-merge-msg.txt:34
#, no-wrap
msgid "--no-log"
msgstr ""

#. type: Plain text
#: ../Documentation/git-fmt-merge-msg.txt:37
msgid "Do not list one-line descriptions from the actual commits being merged."
msgstr ""

#. type: Labeled list
#: ../Documentation/git-fmt-merge-msg.txt:38
#, no-wrap
msgid "--[no-]summary"
msgstr ""

#. type: Plain text
#: ../Documentation/git-fmt-merge-msg.txt:41
msgid ""
"Synonyms to --log and --no-log; these are deprecated and will be removed in "
"the future."
msgstr ""

#. type: Labeled list
#: ../Documentation/git-fmt-merge-msg.txt:42
#, no-wrap
msgid "-m <message>"
msgstr ""

#. type: Labeled list
#: ../Documentation/git-fmt-merge-msg.txt:43
#, no-wrap
msgid "--message <message>"
msgstr ""

#. type: Plain text
#: ../Documentation/git-fmt-merge-msg.txt:46
msgid ""
"Use <message> instead of the branch names for the first line of the log "
"message.  For use with `--log`."
msgstr ""

#. type: Labeled list
#: ../Documentation/git-fmt-merge-msg.txt:47
#, no-wrap
msgid "-F <file>"
msgstr ""

#. type: Labeled list
#: ../Documentation/git-fmt-merge-msg.txt:48
#, no-wrap
msgid "--file <file>"
msgstr ""

#. type: Plain text
#: ../Documentation/git-fmt-merge-msg.txt:51
msgid "Take the list of merged objects from <file> instead of stdin."
msgstr ""

#. type: Title -
#: ../Documentation/git-fmt-merge-msg.txt:53
#, no-wrap
msgid "CONFIGURATION"
msgstr ""

#. type: Labeled list
#: ../Documentation/git-fmt-merge-msg.txt:56
#, no-wrap
msgid "merge.summary"
msgstr ""

#. type: Plain text
#: ../Documentation/git-fmt-merge-msg.txt:59
msgid ""
"Synonym to `merge.log`; this is deprecated and will be removed in the "
"future."
msgstr ""

#. type: Title -
#: ../Documentation/git-fmt-merge-msg.txt:61
#, no-wrap
msgid "EXAMPLES"
msgstr ""

#. type: delimited block -
#: ../Documentation/git-fmt-merge-msg.txt:66
#, no-wrap
msgid ""
"$ git fetch origin master\n"
"$ git fmt-merge-msg --log <$GIT_DIR/FETCH_HEAD\n"
msgstr ""

#. type: Plain text
#: ../Documentation/git-fmt-merge-msg.txt:70
msgid ""
"Print a log message describing a merge of the \"master\" branch from the "
"\"origin\" remote."
msgstr ""

