#: ../Documentation/gitcredentials.txt:297
#, no-wrap
msgid "GIT"
msgstr ""

#. type: Plain text
#: ../Documentation/gitcredentials.txt:298
msgid "Part of the linkgit:git[1] suite"
msgstr ""
