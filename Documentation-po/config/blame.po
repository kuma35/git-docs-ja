#. type: Labeled list
#: ../Documentation/config/blame.txt:1
#, no-wrap
msgid "blame.blankBoundary"
msgstr ""

#. type: Plain text
#: ../Documentation/config/blame.txt:4
msgid ""
"Show blank commit object name for boundary commits in "
"linkgit:git-blame[1]. This option defaults to false."
msgstr ""

#. type: Labeled list
#: ../Documentation/config/blame.txt:5
#, no-wrap
msgid "blame.coloring"
msgstr ""

#. type: Plain text
#: ../Documentation/config/blame.txt:9
msgid ""
"This determines the coloring scheme to be applied to blame output. It can be "
"'repeatedLines', 'highlightRecent', or 'none' which is the default."
msgstr ""

#. type: Labeled list
#: ../Documentation/config/blame.txt:10
#, no-wrap
msgid "blame.date"
msgstr ""

#. type: Plain text
#: ../Documentation/config/blame.txt:14
msgid ""
"Specifies the format used to output dates in linkgit:git-blame[1].  If unset "
"the iso format is used. For supported values, see the discussion of the "
"`--date` option at linkgit:git-log[1]."
msgstr ""

#. type: Labeled list
#: ../Documentation/config/blame.txt:15
#, no-wrap
msgid "blame.showEmail"
msgstr ""

#. type: Plain text
#: ../Documentation/config/blame.txt:18
msgid ""
"Show the author email instead of author name in linkgit:git-blame[1].  This "
"option defaults to false."
msgstr ""

#. type: Labeled list
#: ../Documentation/config/blame.txt:19
#, no-wrap
msgid "blame.showRoot"
msgstr ""

#. type: Plain text
#: ../Documentation/config/blame.txt:22
msgid ""
"Do not treat root commits as boundaries in linkgit:git-blame[1].  This "
"option defaults to false."
msgstr ""

#. type: Labeled list
#: ../Documentation/config/blame.txt:23
#, no-wrap
msgid "blame.ignoreRevsFile"
msgstr ""

#. type: Plain text
#: ../Documentation/config/blame.txt:29
msgid ""
"Ignore revisions listed in the file, one unabbreviated object name per line, "
"in linkgit:git-blame[1].  Whitespace and comments beginning with `#` are "
"ignored.  This option may be repeated multiple times.  Empty file names will "
"reset the list of ignored revisions.  This option will be handled before the "
"command line option `--ignore-revs-file`."
msgstr ""

#. type: Labeled list
#: ../Documentation/config/blame.txt:30
#, no-wrap
msgid "blame.markUnblamableLines"
msgstr ""

#. type: Plain text
#: ../Documentation/config/blame.txt:34
msgid ""
"Mark lines that were changed by an ignored revision that we could not "
"attribute to another commit with a '*' in the output of "
"linkgit:git-blame[1]."
msgstr ""

#. type: Labeled list
#: ../Documentation/config/blame.txt:35
#, no-wrap
msgid "blame.markIgnoredLines"
msgstr ""

#. type: Plain text
#: ../Documentation/config/blame.txt:37
msgid ""
"Mark lines that were changed by an ignored revision that we attributed to "
"another commit with a '?' in the output of linkgit:git-blame[1]."
msgstr ""
