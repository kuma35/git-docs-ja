.SH "SYNOPSIS"
.sp
.nf
\fIgit init\-db\fR [\-q | \-\-quiet] [\-\-bare] [\-\-template=<template\-directory>] [\-\-separate\-git\-dir <git\-dir>] [\-\-shared[=<permissions>]]
.fi
